{
    "manifest_version": 2,
    "name": "Ad Blocker Notice Removal",
    "version": "1.0.0",
    "author": "Jason M",
    "description": "Remove the ad blocker notice on popular sites",
    "minimum_chrome_version": "52",
    "icons": {
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "128": "icon/128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/arrive.min.js", "src/contentscript.js"],
        "run_at": "document_start"
      }
    ],
    "background": {
      "scripts": ["src/menu.js"]
    },
    "browser_action": {
      "default_title": "Ad Blocker Notice Removal"
    },
    "permissions": [
      "contextMenus"
    ],
    "web_accessible_resources": [
      "rules.json"
    ]
  }